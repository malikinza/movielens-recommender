    "#Creating user features to understand user behavior\n",
    "user_features = df1.groupby('userId').agg({'rating': ['mean','std','min','max','count'], \n",
    "                                            'timestamp': ['min','max']}).reset_index()\n",
    "\n",
    "user_features.columns = ['userId', 'user_avg_rating', 'user_rating_std', 'user_min_rating', 'user_max_rating', 'user_n_ratings', \n",
    "                         'user_first_rating_ts', 'user_last_rating_ts']\n",
    "\n",
    "#Calculating user activity span in days\n",
    "user_features['user_activity_span'] = (user_features['user_last_rating_ts'] - user_features['user_first_rating_ts'])/ (24*3600) #converting seconds to days\n",
    "#Calculating user rating velocity\n",
    "user_features['user_rating_velocity'] = user_features['user_n_ratings'] / user_features['user_activity_span'].replace(0, 1)\n",
    "\n",
    "#Finding favorite genre of each user\n",
    "def get_favorite_genre(user_id):\n",
    "    user_data = df1[df1['userId'] == user_id]\n",
    "    genre_counts = {}\n",
    "    for genres in user_data['genres']: #iterating through the user's genre column\n",
    "        for genre in genres.split('|'): #iterating through the genres\n",
    "            genre_counts[genre] = genre_counts.get(genre, 0) + 1 #counting the number of times each genre appears\n",
    "    if genre_counts:\n",
    "        favorite_genre = max(genre_counts, key=genre_counts.get) #returning the genre with the highest count    \n",
    "    return favorite_genre\n",
    "\n",
    "user_features['user_favorite_genre'] = user_features['userId'].apply(get_favorite_genre)\n",
    "\n",
    "print(f\"User features summary: {user_features.describe()}\")\n",
    "\n",
    "#user_features.head(10)\n",
    "\n",
    "#Saving user features to csv\n",
    "user_features.to_csv(OUTPUT_DIR/'user_features.csv', index=False)\n"
   ]
