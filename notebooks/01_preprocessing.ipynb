   "source": [
    "#User-stratified train test split - for each user, split their ratings into train and test sets based on the timestamp\n",
    "#This ensure 0% cold start users in the test set\n",
    "\n",
    "#80/20 time based split\n",
    "train_list = []\n",
    "test_list = []\n",
    "skipped_users = 0\n",
    "\n",
    "for user_id, user_data in df1.groupby('userId'):\n",
    "    user_sorted = user_data.sort_values(by='timestamp')\n",
    "\n",
    "    split_index = int(0.8 * len(user_sorted))\n",
    "\n",
    "    if split_index > 0:\n",
    "        train_list.append(user_sorted.iloc[:split_index])\n",
    "        test_list.append(user_sorted.iloc[split_index:])\n",
    "    else:\n",
    "        skipped_users += 1\n",
    "    \n",
    "\n",
    "df_train = pd.concat(train_list)\n",
    "df_test = pd.concat(test_list)"
   ]
