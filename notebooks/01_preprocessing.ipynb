   "source": [
    "#Filtering thresholds to remove outliers and very casual users\n",
    "#Keeping only engaged users who have rated enough movies\n",
    "#Keeping only movies that have enough ratings to be meaningful\n",
    "\n",
    "#99th percentile of users and movies\n",
    "user_99th = user_count.quantile(0.99)\n",
    "movie_99th = movie_count.quantile(0.99)\n",
    "\n",
    "#Set thresholds\n",
    "min_user_ratings = 10 #Keeping only users who have rated at least 10 movies [conservative approach]\n",
    "max_user_ratings = user_99th\n",
    "\n",
    "min_movie_ratings = 10 #Keeping only movies that have been rated at least 10 times [conservative approach]\n",
    "max_movie_ratings = movie_99th\n",
    "\n",
    "#Iteratively filter users and movies with min and max ratings\n",
    "#Iterative filtering because when we remove users, some movies might fall below the threshold and vice versa\n",
    "df1 = df.copy()\n",
    "iteration = 0\n",
    "\n",
    "while True:\n",
    "    iteration += 1\n",
    "    initial_len = len(df1)\n",
    "\n",
    "    #valid users\n",
    "    valid_users = user_count[(user_count >= min_user_ratings) & (user_count <= max_user_ratings)].index\n",
    "    df1 = df1[df1['userId'].isin(valid_users)]\n",
    "\n",
    "    #valid movies\n",
    "    valid_movies = movie_count[(movie_count >= min_movie_ratings) & (movie_count <= max_movie_ratings)].index\n",
    "    df1 = df1[df1['movieId'].isin(valid_movies)]\n",
    "\n",
    "    #Check convergence\n",
    "    if len(df1) == initial_len:\n",
    "        print(f\"Converged at iteration {iteration}\")\n",
    "        break\n",
    "\n",
    "\n",
    "#Sanity check - number of ratings should be the same\n",
    "print(f\"Number of ratings before filtering: {len(df)}\")\n",
    "print(f\"Number of ratings after filtering: {len(df1)}\")\n",
    "print(f\"Percentage of retained ratings: {(len(df1) / len(df)) * 100}%\")"
   ]
  },
