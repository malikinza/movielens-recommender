    "#Creating movie features\n",
    "\n",
    "movie_features = df1.groupby('movieId').agg({'rating': ['mean','std','min','max','count'], \n",
    "                                              'timestamp': ['min','max'],\n",
    "                                              'userId': 'nunique',\n",
    "                                              'title': 'first',\n",
    "                                              'genres': 'first',\n",
    "                                              'release_year': 'first'}).reset_index()\n",
    "\n",
    "movie_features.columns = ['movieId', 'movie_avg_rating', 'movie_rating_std', 'movie_min_rating', 'movie_max_rating', 'movie_rating_count', \n",
    "'movie_first_rating_ts', 'movie_last_rating_ts', 'movie_unique_users', 'movie_title', 'movie_genre', 'movie_release_year']\n",
    "\n",
    "#Calculate rating span in days\n",
    "movie_features['movie_rating_span'] = (movie_features['movie_last_rating_ts'] - movie_features['movie_first_rating_ts'])/ (24*3600) #converting seconds to days\n",
    "\n",
    "#Calculate rating velocity\n",
    "movie_features['movie_rating_velocity'] = movie_features['movie_rating_count'] / movie_features['movie_rating_span'].replace(0, 1)\n",
    "\n",
    "#Popularity score\n",
    "movie_features['movie_popularity_score'] = movie_features['movie_rating_count'] / movie_features['movie_rating_span']\n",
    "\n",
    "#Movie weighted rating \n",
    "movie_features['movie_weighted_rating'] = (movie_features['movie_rating_count']*movie_features['movie_avg_rating'])/ (movie_features['movie_rating_count'] + 1)\n",
    "\n",
    "print(f\"Movie features summary: {movie_features.describe()}\")\n",
    "\n",
    "movie_features.head(10)\n",
    "\n",
    "#Saving movie features to csv\n",
    "movie_features.to_csv(OUTPUT_DIR/'movie_features.csv', index=False)"
   ]
