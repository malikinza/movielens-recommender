    "#Encoding IDs\n",
    "\n",
    "#Creating mapping dictionaries for user and movie IDs\n",
    "unique_users = df1['userId'].unique()\n",
    "unique_movies = df1['movieId'].unique()\n",
    "\n",
    "user_id_to_index = {user_id: index for index, user_id in enumerate(unique_users)}\n",
    "movie_id_to_index = {movie_id: index for index, movie_id in enumerate(unique_movies)}\n",
    "\n",
    "df1['user_index'] = df1['userId'].map(user_id_to_index)\n",
    "df1['movie_index'] = df1['movieId'].map(movie_id_to_index)\n",
    "\n",
    "#Creating reverse mappings\n",
    "index_to_user_id = {index: user_id for user_id, index in user_id_to_index.items()}\n",
    "index_to_movie_id = {index: movie_id for movie_id, index in movie_id_to_index.items()}\n",
    "\n",
    "#Saving encoding\n",
    "encodings = {\n",
    "    'user_id_to_index': user_id_to_index,\n",
    "    'movie_id_to_index': movie_id_to_index,\n",
    "    'index_to_user_id': index_to_user_id,\n",
    "    'index_to_movie_id': index_to_movie_id,\n",
    "    'n_users': len(unique_users),\n",
    "    'n_movies': len(unique_movies)\n",
    "}\n",
    "\n",
    "#Saving encodings to pickle\n",
    "with open(OUTPUT_DIR/'encodings.pkl', 'wb') as f:\n",
    "    pickle.dump(encodings, f)"
   ]
